{% extends 'base.html.twig' %}

{% block body %}
    <h1>GeneralSkill</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ generalSkill.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ generalSkill.type }}</td>
            </tr>
            <tr>
                <th>Level</th>
                <td>{{ generalSkill.level }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ generalSkill.description }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('generalskill_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('generalskill_edit', { 'id': generalSkill.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
