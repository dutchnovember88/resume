{% extends 'base.html.twig' %}

{% block body %}
    <h1>SpecificSkill</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ specificSkill.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ specificSkill.type }}</td>
            </tr>
            <tr>
                <th>Category</th>
                <td>{{ specificSkill.category }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('specificskill_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('specificskill_edit', { 'id': specificSkill.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
